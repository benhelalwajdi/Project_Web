{% extends 'template.html.twig' %}
{% block body %}

    {% block content %}
        {% block menu %}
            <section class="pageTitleSection">
                <div class="container">
                    <div class="pageTitleInfo">
                        <h2>Education</h2>
                        <ol class="breadcrumb">
                            <li><a href="index-2.html">Accueil</a></li>
                            <li class="active">Email</li>
                        </ol>
                    </div>
                </div>
            </section>
        {% endblock  %}
        <br>
        <br>
        <div class="container"  style="margin:auto ">

            <div class="row">
                <div class="col-xs-12" >
                    <div class="tabCommon">

                        <div class="tab-content" style="width: 1200px; margin-right: 10px">
                            <div id="leftHome" class="tab-pane fade in active" style="margin-right: 10px">
                                <div class="media">
                                    <div class="container" >
                                        <div class="sectionTitle text-center" style="font-size: small">

                                            <h2>

                                                <span class="shape shape-left bg-color-4"></span>
                                                <span >     <a >Email </a></span>
                                                <span class="shape shape-right bg-color-4"></span>
                                            </h2>
                                        </div>

                                        <section class="mainContent full-width clearfix">
                                            <div class="container">



                                                {{ form_start(form) }}


                                                <div class="form-group formField">
                                                    {{ form_widget(form.nom, { 'attr': {'class': "form-control",
                                                        'placeholder': "DONNER NOM "|trans} }) }}
                                                </div>
                                                <div class="form-group formField">
                                                    {{ form_widget(form.prenom, { 'attr': {'class': "form-control",
                                                        'placeholder': "DONNER PRENOM"|trans} }) }}
                                                </div>
                                                <div class="form-group formField">
                                                    {{ form_widget(form.tel, { 'attr': {'class': "form-control",
                                                        'placeholder': "NUMERO TEL"|trans} }) }}
                                                </div>
                                                <div class="form-group formField">
                                                    {{ form_widget(form.email, { 'attr': {'class': "form-control",
                                                        'placeholder': "L'EMAIL"|trans} }) }}
                                                </div>
                                                <div class="form-group formField">
                                                    {{ form_widget(form.text, { 'attr': {'class': "form-control",
                                                        'placeholder': "CONTENU DU MSG"|trans} }) }}
                                                </div>
                                                <div class="form-group formField">
                                                    {{ form_widget(form.valider, { 'attr': {'class': "btn btn-primary btn-block bg-color-3 border-color-3",
                                                        'placeholder': "VOTRE_NUM TEL"|trans} }  ) }}
                                                </div>


                                                {{ form_end(form) }}

                                            </div>
                                        </section>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br>
        <br>
    {% endblock %}
    {% block script1 %}
        {{ parent() }}
    {% endblock %}

{% endblock %}