{% for m in bs  %}<link rel="script" href="{{ asset('gmaps/gmaps.js') }}"><link rel="stylesheet" type="text/css" href="{{ asset('examples.css') }}" />{% block content %}    <div class="container">        <div class="timeline">            <div class="timeline-cover">                <!--Timeline Menu for Large Screens-->                <div class="timeline-nav-bar hidden-sm hidden-xs">                    <div class="row">                            <div class="profile-info">                                <img src="{{ asset(vich_uploader_asset(m, 'imageFile')) }}" alt=""                                     class="img-responsive profile-photo"/>                                <h3>{{ m.prenom }} {{ m.nom }}</h3>                                <p class="text-muted">@{{ m.username }}</p>                            </div>                    </div>                </div><!--Timeline Menu for Large Screens End-->                <form class="navbar-form navbar-right hidden-sm" action="{{ path('user_find') }}" method="post">                    <div class="form-group">                        <i class="icon ion-android-search"></i>                        <input type="text" name="rech" class="form-control" placeholder="Rechercher...">                    </div>                </form>                <!--Timeline Menu for Small Screens-->                <div class="navbar-mobile hidden-lg hidden-md">                    <div class="profile-info">                        <h4>{{ m.prenom }} {{ m.nom }}</h4>                        <p class="text-muted">@{{ m.username }}</p>                    </div>                </div><!--Timeline Menu for Small Screens End-->            </div>            <div id="page-contents">                <center>                    <div class="col-md-7 col-sm-7">                       {% if m in abonnement  %}                            <a href="{{ path('desabonner', {'id': m.id}) }}">                                <button  class="btn btn-warning btn-sm pull-right " color><i class="ion-close-round"></i>&nbsp;<b>Se désabonner </b></button>                            </a>                       {% elseif m not in abonnement %}                            <a href="{{ path('sabonner', {'id': m.id}) }}">                                <button class="btn-primary pull-right "><i class="ion-plus-round"></i>&nbsp;S'abonner</button>                            </a>                       {% endif %}                    </div>                    <a href="{{ path('AjoutBesoin', {'id': m.id}) }}">                        <button type="submit" class="btn btn-primary"><span                                    class="glyphicon glyphicon-send"></span>&nbsp; Ajouter besoin </button>                    </a>                    <a href="{{ path('discussion', {'id': m.id}) }}">                        <button type="submit" class="btn btn-primary"><span                                    class="glyphicon glyphicon-send"></span>&nbsp; Envoyer un message</button>                    </a>                    <a href=" {{ asset(vich_uploader_asset(m, 'cvFile')) }}">                        <button class="btn-primary pull-right "><i class="ion-ios-cloud-download"></i>&nbsp;Télécharger                            le CV                        </button>                    </a><br>                    <br>                    <div>                        <h4 style="color: #1a7bba">Nom : </h4>{{ m.nom }}                        <h4 style="color: #1a7bba">Prenom : </h4>{{ m.prenom }}                        <h4 style="color: #1a7bba">Adresse Mail : </h4>{{ m.email }}                        <h4 style="color: #1a7bba">Ville : </h4>{{ m.ville }}<br>                        <h4 style="color: #1a7bba">Pour envoyer e-mail : </h4><img src="{{ qrcode_data_uri( m.email , { writer: 'svg', size: 100 }) }}" />                        <h4 style="color: #1a7bba">Salair par heure</h4> {{ m.salaire }} dinar                        <br>                        <input id="nbr" type="text" placeholder="exemple nombre de jours" size="30px" value="0">                        <select id="salaire" onchange="calcule_frais()">                            <option value="0">votre choix !</option>                            <option value="1">heure</option>                            <option value="2">jour</option>                            <option value="3">semaine</option>                            <option value="4">mois</option>                        </select>                        <div id="map"></div>                    </div>                </center>            </div>        </div>    </div>    {% block js %}        <script>            function calcule_frais() {                $nbr = document.getElementById("nbr").value;                $s= document.getElementById("salaire").value;                if($nbr==0)                {alert ("le nombre d'heure ou bien le nombre de jours doits etre supérieur a 0 ")}                else if($s==1){                    if(($nbr>=1)&&($nbr<=8)){                        $frais = parseFloat({{ m.salaire }}) ;                        $frais = ($frais * $nbr);                        alert("les frais pendent "+ $nbr +" heurs est : "+ $frais +" dinars");                    }else {                        alert("nombre d'heure doit etre entre 1 heure et 8 heurs !! ")                    }                }                else if($s==2){                    if(($nbr>=1)&&($nbr<=6)){                        $frais = parseFloat({{ m.salaire }}) ;                        $frais = ($frais * (8 * $nbr));                        alert("les frais pendent "+ $nbr +" jours est : "+ $frais +" dinars");                    }else {                        alert("nombre de jours doit etre entre 1 jour et 6 jours !! ")                    }                }                else if($s==3){                    if(($nbr>=1)&&($nbr<=4)){                        $frais = parseFloat({{ m.salaire }}) ;                        $frais = ($frais * (( $nbr*6 ) * 8) );                        alert("les frais pendent "+ $nbr +" semaine est : "+ $frais +" dinars");                    }else {                        alert("nombre d'semaine doit etre entre une semaine et 4 semaines !! ")                    }                }                else if($s==4){                    if(($nbr>=1)&&($nbr<=12)){                        $frais = parseFloat({{ m.salaire }}) ;                        $frais = ($frais * ((($nbr*4)*6) * 8) );                        alert("les frais pendent "+ $nbr +" mois est : "+ $frais +" dinars");                    }else {                        alert("nombre de mois doit etre entre une mois et 12 mois !! ")                    }                }            }        </script>        <script src="{{ asset('Public/js/jquery-3.1.1.min.js') }}"></script>        <script src="{{ asset('Public/js/bootstrap.min.js') }}"></script>        <script src="{{ asset('Public/js/jquery.appear.min.js') }}"></script>        <script src="{{ asset('Public/js/jquery.incremental-counter.js') }}"></script>        <script src="{{ asset('Public/js/script.js') }}"></script>        <script src="{{ asset('Public/css/jquery.scrollbar.css') }}"></script>        <script src="{{ asset('Public/js/jquery.sticky-kit.min.js') }}"></script>        <script>            var map;            var markers = [];            var marker;            function initMap() {                if (navigator.geolocation) { //Checks if browser supports geolocation                    navigator.geolocation.getCurrentPosition(function (position) {                                                              //This gets thevar latitude = position.coords.latitude;                    //users current                        var longitude = position.coords.longitude;                        var latitude = position.coords.latitude;                        var coords = new google.maps.LatLng(latitude, longitude); //Creates variable for map coordinates                        var directionsService ;                        var directionsDisplay ;                        var b =true ;                        var mapOptions = //Sets map options                            {                                zoom: 15,  //Sets zoom level (0-21)                                center: coords, //zoom in on users location                                mapTypeControl: true, //allows you to select map type eg. map or satellite                                navigationControlOptions:                                    {                                        style: google.maps.NavigationControlStyle.SMALL //sets map controls size eg. zoom                                    },                                mapTypeId: google.maps.MapTypeId.ROADMAP //sets type of map Options:ROADMAP, SATELLITE, HYBRID, TERRIAN                            };                        map = new google.maps.Map(document.getElementById("map"), mapOptions);                        marker = new google.maps.Marker({                            position: coords,                            map: map,                            title : "thats me"                        });                        marker = new google.maps.Marker({                            position: {lat: parseFloat({{ m.lat }}), lng: parseFloat({{ m.lag }})},                            map: map,                            title: "Drag me!"                        });                        var content = "adresse email : {{ m.email }} </br>" + "" +                            "nom : {{ m.nom }}" + "</br>" +                            "prenom : {{ m.prenom }}";                        var infowindow = new google.maps.InfoWindow();                        google.maps.event.addListener(marker, 'click',                            (function (marker, content, infowindow) {                                return function () {                                    infowindow.setContent(content);                                    infowindow.open(map, marker);                                };                            })(marker, content, infowindow));                        markers.push(marker);                        google.maps.event.addListener( marker, 'click', function(e){                            if (!b) {                                directionsDisplay.setMap(null);                            }                            directionsService = new google.maps.DirectionsService();                            directionsDisplay = new google.maps.DirectionsRenderer();                            directionsDisplay.setMap(map);                            directionsDisplay.setPanel(document.getElementById('map'));                            var request = {                                origin: coords,                                destination: {lat: this.position.lat(), lng: this.position.lng()},                                travelMode: google.maps.DirectionsTravelMode.DRIVING,                            };                            directionsService.route(request, function (response, status) {                                if (status == google.maps.DirectionsStatus.OK) {                                    directionsDisplay.setDirections(response);                                }                            });                            b = false ;                        });                        /*map.addListener('click', function(event) {                            addMarker(event.latLng);                        });*/                    });                }            }        </script>        <script async defer src="{{ asset('https://maps.googleapis.com/maps/api/js?key=AIzaSyAuFusBAC3vYNl0ye8bXOk0nGhGUo109qE&callback=initMap') }}">        </script>    {% endblock %}    {% block stylesheets %}        <link href="{{ asset('Public/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" media="all"/>        <link href="{{ asset('Public/css/style.css') }}" rel="stylesheet" type="text/css" media="all"/>        <link href="{{ asset('Public/css/ionicons.min.css') }}" rel="stylesheet" type="text/css" media="all"/>        <link href="{{ asset('Public/css/style.css') }}" rel="stylesheet" type="text/css" media="all"/>        <link href="{{ asset('Public/css/font-awesome.min.css') }} " rel="stylesheet" type="text/css" media="all"/>        <link href='https://fonts.googleapis.com/css?family=Raleway:400,100,100italic,200,200italic,300,300italic,400italic,500,500italic,600,600italic,700' rel='stylesheet' type='text/css'>    {% endblock %}{% endblock %}{% endfor %}