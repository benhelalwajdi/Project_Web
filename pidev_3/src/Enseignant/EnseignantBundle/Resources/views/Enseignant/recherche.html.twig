                        <div class="col-xs-12" >
                            <div class="tabCommon">

                                <div class="sectionTitle text-center" style="font-size: small">

                                    <h2>

                                        <span class="shape shape-left bg-color-4"></span>
                                        <span> Enseignants
                                               </span>
                                        <span class="shape shape-right bg-color-4"></span>
                                    </h2>
                                </div>
                                <section class="mainContent full-width clearfix coursesSection">
                                    <div class="container">
                                        <div class="row">

                                            <div class="col-md-9 col-sm-8 col-xs-12 pull-right">

                                                <div class="row">
                                                    {% for ligne in m %}
                                                        <div class="col-md-4 col-sm-6 col-xs-12 block">

                                                            <div class="thumbnail thumbnailContent">
                                                               <img src="{{ asset('images/') }}{{ ligne.nomimage }}" alt="image" class="img-responsive" style="width: 303px;height: 200px;">
                                                                <div class="caption border-color-1">
                                                                    <h3><a href="=" class="color-1">{{ ligne.nom }} {{ ligne.prenom }}</a></h3>
                                                                    <ul class="list-inline" style="font-weight: bold">
                                                                        <li>{{ ligne.adresse }}</li><br>
                                                                        <li> {{ ligne.special }}</li>
                                                                    </ul>
                                                                    <p>{{ ligne.email }}<br>Numero de téléphone: {{ ligne.numtel }}<br>Prix par heure:<a style="font-weight: bold;color: #4cae4c;">{{ ligne.prix }}</a>  <br> Nbre d'heure de formation: <a style="font-weight: bold;color: #4cae4c;">{{ ligne.nbr }}</a> </p>
                                                                    <ul class="list-inline btn-yellow">
                                                                        <li><a href="{{ path('calculer',{'id':ligne.id}) }}"class="btn btn-primary ">calculer</a></li>
                                                                    </ul>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    {% endfor %}
                                                </div>
                                            </div>

                                            <div class="col-md-3 col-sm-4 col-xs-12 pull-left">
                                                <aside>
                                                    <div class="panel panel-default courseSidebar">
                                                        <div class="panel-heading bg-color-1 border-color-1">
                                                            <h3 class="panel-title">Recherche</h3>
                                                        </div>
                                                        <div class="panel-body">
                                                            <div class="form-group formField">
                                                                <div class="dropGeneral roundOne">
                                                                    <form method="post">
                                                                        <input type="text" class="form-control" placeholder="nom"  name="nom" >
                                                                        <div style="margin-right: 0px;"><input class="btn btn-warning " type="submit" value="Chercher"  name="cherhcer" >  </div>                                                              </form>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        </br>
                                                    </div>
                                                </aside>
                                            </div>
                                        </div>


                                    </div>
                                </section>


                            </div>
                        </div>
                    </div>
                </div>
                <br>
                <br>
            {% endblock %}
            {% block script1 %}
                {{ parent() }}
            {% endblock %}



        </section>
